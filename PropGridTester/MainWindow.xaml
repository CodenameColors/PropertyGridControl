<Window x:Class="PropGridTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PropGridTester"
				xmlns:mm="clr-namespace:PropertyGridEditor;assembly=PropertyGridEditor"
				
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
	<Grid Background="Black">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="100"/>
		</Grid.RowDefinitions>

		<mm:PropGrid Name="ttt">
			
		</mm:PropGrid>

		<Button Grid.Row="1" Height="30" Width="70" Content="Testing!" Click="Button_Click"/>

	</Grid>
</Window>
